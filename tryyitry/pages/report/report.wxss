page{
  height: 100%;
  background: #5599b0
}

.canvas {
  width: 100%;
  height: 550rpx;
  color: #ffffff;
}
.report-top{
  width: 100%;
  height: 100rpx;
  background: #236f7f;
  overflow: hidden;
}
.check-top-left{
  color: #ffffff;
  line-height: 100rpx;
  font-size: 32rpx;
  width: 70%;
  height: 100%;
  float: left;
}
.check-top-left image{
  width: 25rpx;
  height: 25rpx;
  float: left;
  margin-top:45rpx;
  margin: 45rpx 10rpx 0
}
.check-top-left text{
  float: left;
  display: block;
}
.index-top-left1{
  float: left;
  margin-left: 20rpx;
}
.index-top-left2{
  float: left;
   margin-left: 10rpx;
}
.check-top-right{
  float: right;
}
.check-top-right text{
  line-height: 100rpx;
  font-size: 32rpx;
  margin: 0rpx 10rpx;
  color: #fffd8a
}
.check-top-right text:nth-child(2){
  color: #ffffff
}
.index-tap{
  width: 50%;
  height: 80rpx;
  background: #3b92a3;
  margin: 50rpx auto;
  border-radius: 50rpx;
  border: 2rpx solid #6abdd1;
  overflow: hidden;
}
.idenx-tap-btn{
  width: 45%;
  height: 90%;
  float: left;
  text-align: center;
  margin-left: 5%;
  overflow: hidden;
  margin-top: 1%;
  color: #f2f2f2;
  line-height: 100%;
}
.index-tap-btn-active{
background: #225d65;
border-radius: 50rpx;
}
.idenx-tap-btn:nth-child(2){
  margin-left: 0rpx;
}
.idenx-tap-btn text{
  display: block;
  margin-top: 15rpx;
}
.report-bottom{
  width: 100%;
  margin-top: 40rpx;
}
.report-bottom-top{
  width: 100%;
  height: 80rpx;
  background: #74b0c8;
  font-size: 34rpx;
  line-height: 80rpx;
  text-align: center;
  color: #ffffff
}
.report-bottom-bottom{
  width: 100%;
  background: #7d9da8;
  overflow: hidden
}
.report-bottom-lists{
  width: 49%;
  height: 70rpx;
  border-bottom: 2rpx solid #abaeaf;
  float: left;
  font-size: 30rpx;
  color: #ffffff;
  line-height: 70rpx;
}
.report-bottom-lists text{
  float: left;
  width: 50%;
  text-align: center;
}
.report-bottom-lists:nth-child(odd){
  border-right: 2rpx solid #abaeaf;
}
.report-bottom-lists:nth-child(even){
  float: right;
}
.check-bottom{
  position: relative
}
.photo{
  width: 100%
}
.photo2{
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.cur{
  display: none;
}