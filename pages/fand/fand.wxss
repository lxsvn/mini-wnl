/* pages/fand/fand.wxss */
page{
  background: #f2f2f2;
}
.content{
  overflow: hidden;
}
.fand{
  width: 100%;
  margin: 40rpx auto 0;
  position: relative;
  overflow: hidden;
}
.content-img{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1
}
.fand-top{
  width: 90%;
  margin: 0 auto 0;
  position: relative;
  padding-bottom: 5rpx;
}
.fand-topimg1{
  width: 120rpx;
  height: 120rpx;
  display: block;
  margin: 80rpx auto 0
}
.fand-topimg12{
  position: absolute;
  bottom: 0;
  width: 90%;
  height: 50rpx;
  z-index: -1;
  left: 50%;
  margin-left: -45%;
}
.fand-top text{
  margin-top: 25rpx;
}
.fand-top text:nth-child(3){
  font-size: 35rpx;
  color: #ffffff;
 
}
.fand text{
  display: block;
}
.fand-top{
   text-align: center;
}
.fand-center {
  font-size: 32rpx;
  margin-top: 50rpx;
  text-align: center;
}
.fand-center text{
  margin: 5rpx 0
}
.fand-bottom{
  width: 80%;
  border-top: 1px solid #eccc76;
  margin: 80rpx auto 100rpx;
  overflow: hidden;
}
.fand-bottom-lists{
  float: left;
  margin-top: 30rpx;
  margin-right: 25rpx
}
.fand-bottom-lists image{
    width: 150rpx;
    height: 150rpx;
    margin-top: 20rpx
}
.fand-text text{
  display: block;
  text-align: center;
  font-size: 30rpx;
  margin-top: 10rpx
}
.fand-text text:nth-child(2){
  color: #999;
  font-size:26rpx;
  margin-top: 5rpx
}
.fand-bottom-lists:nth-child(1) text{
  font-size: 30rpx;
  line-height: 60rpx
}
.fand-bottom-lists:nth-child(2) text{
  font-size: 32rpx;
  line-height: 60rpx;
  color: #999
}
.fand-bottom-lists:nth-child(3) {
 margin-right: 0rpx ;
 float: right
 
}
.fand-bottom-lists:nth-child(3) text {
 font-size: 30rpx;
 color: #666
 
}
.fand-text{
  text-align: center;
  width: 100%;
  height: 100rpx;
  margin: 30rpx 0
}